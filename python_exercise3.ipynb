   "source": [
    "st = input(\"Please enter string: \")\n",
    "n = len(st)\n",
    "i = 0\n",
    "while i < n- 1:\n",
    "    # Count occurrences of\n",
    "    # current character\n",
    "    count = 1\n",
    "    while (i < n - 1 and\n",
    "           st[i] == st[i + 1]):\n",
    "        count += 1\n",
    "        i += 1\n",
    "    i += 1\n",
    "    print(st[i - 1] + str(count), end = \"\")"
   ]
